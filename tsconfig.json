{
  "compilerOptions": {
    "target": "es6",
    "module": "es2015",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./", 
    "paths": {
      "*": ["node_modules/*", "src/*"],
      "models/*": ["src/models/*"]
    }
  }
}
